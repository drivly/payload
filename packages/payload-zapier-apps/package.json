{
  "name": "payload-zapier-apps",
  "version": "0.1.0",
  "type": "module",
  "description": "Zapier app generator for Payload CMS collections",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "payload",
    "cms",
    "zapier",
    "integration"
  ],
  "author": "Drivly",
  "license": "MIT",
  "homepage": "https://mdx.org.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/drivly/ai.git",
    "directory": "pkgs/payload-zapier-apps"
  },
  "bugs": {
    "url": "https://github.com/drivly/ai/issues"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint-config": "0.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0",
    "tsconfig": "0.0.0"
  },
  "peerDependencies": {
    "payload": "^3.0.0"
  }
}
